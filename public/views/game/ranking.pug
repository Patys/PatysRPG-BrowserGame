div#panel-ranking
  .panel.panel-primary
    .panel-heading
      h3.panel-title Ranking
    .panel-body
      div
        .row(style="border-bottom: 1px solid #d3d3d3; margin-left: 15px; margin-right: 15px;")
          .col-sm-4
            p Nazwa
          .col-sm-2
            p Siła
          .col-sm-2
            p Witalność
          .col-sm-2
            p Inteligencja
          .col-sm-2
            p Zreczność
        - if (typeof(game_data) !== 'undefined'){
        each rank in game_data.ranking
          .row(style="border-bottom: 1px solid #d3d3d3; margin-left: 15px; margin-right: 15px;")
            .col-sm-4
              p= rank.name
            .col-sm-2
              p= rank.strength
            .col-sm-2
              p= rank.vitality
            .col-sm-2
              p= rank.inteligence
            .col-sm-2
              p= rank.agility
        .row-fluid
          label(for='pagination-ranking') Strona &nbsp;
          input#pagination-ranking.form-control(name='page', type='number', placeholder='Strona', val="1")
          button.btn.btn-default#ranking-get Przejdź

        //- nav(aria-label='Page navigation')
