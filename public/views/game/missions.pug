div#panel-mission
  .panel.panel-primary
    .panel-heading
      h3.panel-title Misje
    .panel-body
      div
        // Nav tabs
        ul.nav.nav-tabs(role='tablist')
          li.active(role='presentation')
            a(href='#tab-missions', aria-controls='tab-missions', role='tab', data-toggle='tab') Misje
          li(role='presentation')
            a(href='#tab-active', aria-controls='tab-active', role='tab', data-toggle='tab') Aktywne
        // Tab panes
        .tab-content
          #tab-missions.tab-pane.active(role='tabpanel')
            include missionspanel.pug
          #tab-active.tab-pane(role='tabpanel')
            include missionsactive.pug
